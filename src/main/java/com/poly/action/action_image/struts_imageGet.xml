<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
 "http://struts.apache.org/dtds/struts-2.1.dtd">

<struts>

	<!-- Include webwork defaults (from WebWork JAR). -->
	<!-- <include file="struts-default.xml" /> -->

	<package name="struts_imageGet" extends="myDefault">
	
		<action name="imageInit" class="com.poly.action.action_image.ImageGetAction" method="imageInit">
			<result name="success" type="freemarker">/WEB-INF/ftl/common_imageGet.ftl</result>
		</action>
		
		<action name="getImageInfo" class="com.poly.action.action_image.ImageGetAction" method="getImageInfo">
		</action>
		
		<action name="getImageNotInfo" class="com.poly.action.action_image.ImageGetAction" method="getImageNotInfo">
		</action>
		
	</package>
	
	

</struts>